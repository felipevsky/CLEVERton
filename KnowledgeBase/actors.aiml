    </template>
</category>

<!-- Win/Lost oscar in this year templates (based on "OscarWinnerThisYear" variable) -->
<category> <pattern>DID HE WIN</pattern>
    <template>
        <condition name = "OscarWinnerThisYear" value = "yes">
